<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="seq_appointment_op_number" model="ir.sequence">
        <field name="name">Appointment OP Number Sequence</field>
        <field name="code">appointment.op_number</field>
        <field name="prefix">EA-</field>
        <field name="number_increment">1</field>  <!-- Increment by 1 for each new reference -->
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
